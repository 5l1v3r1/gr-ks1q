<?xml version="1.0"?>
<block>
  <name>KS-1Q FEC decoder</name>
  <key>ks1q_fec_decoder</key>
  <category>[ks1q]</category>
  <import>import ks1q</import>
  <make>ks1q.fec_decoder($verbose)</make>

  <param>
    <name>Verbose</name>
    <key>verbose</key>
    <type>bool</type>
     <option>
       <name>Yes</name>
       <key>True</key>
     </option>
     <option>
       <name>No</name>
       <key>False</key>
     </option>
  </param>

  <sink>
    <name>in</name>
    <type>message</type>
  </sink>

  <source>
    <name>out</name>
    <type>message</type>
  </source>
</block>
